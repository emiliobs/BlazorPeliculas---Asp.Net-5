@page "/counter"


<h1>Counter</h1>

<p>Current count: @currentCount</p>
<p>Singleton count: @ServiciosSingleton.Valor</p>
<p>Transient count: @ServiciosTransient.Valor</p>



<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>
<button class="btn btn-primary" @onclick="IncrementoCountJavascript">Click me Form Javascript.</button>


